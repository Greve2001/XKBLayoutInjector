// Hybrid Coding Layout Danish/English

default partial alphanumeric_keys modifier_keys
xkb_symbols "hybrid" {

    include "dk(basic)"

    name[Group1] = "Coding Hybrid";
    
    key <AC10>	{[	parenleft,	braceleft,	bracketleft,	bracketleft]};
    key <AC11>	{[	parenright,	braceright,	bracketright,	bracketright]};

    include "eurosign(5)"
};






// EXTRAS:

partial alphanumeric_keys
	xkb_symbols "sun_type6" {
	include "sun_vndr/dk(sun_type6)"
};
